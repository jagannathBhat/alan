{
  "manifest_version": 3,
  "name": "alan",
  "version": "0.1.0",
  "description": "alan is a chrome extension that records the user's screen and audio",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "action": {
    "default_icon": {
      "16": "icons/icon16.png",
      "24": "icons/icon24.png",
      "32": "icons/icon32.png"
    },
    "default_popup": "index.html"
  }
}
